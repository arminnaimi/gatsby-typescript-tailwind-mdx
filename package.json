{
  "name": "gatsby-typescript-tailwind-mdx",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/arminnaimi/gatsby-typescript-tailwind-mdx"
  },
  "bugs": {
    "url": "https://github.com/impulse/gatsby-typescript-tailwind/issues"
  },
  "author": "Armin Naimi <armin.naimi@gmail.com>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "typescript",
    "tailwind",
    "tailwind css",
    "starter",
    "markdown",
    "mdx"
  ],
  "scripts": {
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md,ts,tsx}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.6",
    "@mdx-js/react": "^1.6.6",
    "gatsby-background-image": "^1.1.1",
    "gatsby-cli": "^2.12.60",
    "gatsby-plugin-mdx": "^1.2.25",
    "gatsby-plugin-nprogress": "^2.3.10",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^3.10.0"
  },
  "devDependencies": {
    "@types/mdx-js__react": "^1.5.2",
    "@types/react": "^16.9.42",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "autoprefixer": "^9.8.5",
    "babel-eslint": "^10.1.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.4.0",
    "eslint-config-react-app": "^5.2.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.7",
    "gatsby": "^2.24.2",
    "gatsby-image": "^2.4.13",
    "gatsby-plugin-eslint": "^2.0.8",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-manifest": "^2.4.18",
    "gatsby-plugin-offline": "^3.2.17",
    "gatsby-plugin-postcss": "^2.3.11",
    "gatsby-plugin-purgecss": "^5.0.0",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-robots-txt": "^1.5.1",
    "gatsby-plugin-sharp": "^2.6.19",
    "gatsby-plugin-sitemap": "^2.4.11",
    "gatsby-plugin-typescript": "^2.4.14",
    "gatsby-remark-figure-caption": "^2.0.0",
    "gatsby-remark-images": "^3.3.18",
    "gatsby-remark-relative-images": "^0.3.0",
    "gatsby-remark-unwrap-images": "^1.0.2",
    "gatsby-source-filesystem": "^2.3.19",
    "gatsby-transformer-sharp": "^2.5.11",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "tailwindcss": "^1.4.6",
    "typescript": "^3.9.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
